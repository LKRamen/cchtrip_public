{
  "headers": [
    {
      "source": "/(.*)\\.(css|js|jpg|jpeg|gif|png|svg|txt|ttf|woff2|webmanifest)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, s-maxage=600, must-revalidate"
        }
      ]
    }
  ]
}
